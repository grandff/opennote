# OpenAI API Key (현재는 백엔드에서 사용)
VITE_OPENAI_API_KEY=sk-your-api-key-here

# Backend API URL (ngrok 또는 프로덕션 URL)
VITE_BACKEND_API_URL=http://localhost:8080

# Extension API Key (백엔드에서 발급받은 키)
VITE_EXTENSION_API_KEY=your-extension-api-key-here

# Firebase Configuration
# Firebase Console > Project Settings > General > Your apps > Config에서 가져오기
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id
# VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id (선택사항)
